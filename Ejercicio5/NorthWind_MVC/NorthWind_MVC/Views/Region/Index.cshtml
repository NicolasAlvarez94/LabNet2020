@model List<Entidades.Region>
@{
    ViewBag.Title = "Index";
}
<link href="~/Content/RegionStyles.css" rel="stylesheet" />


<h2 class="bg-dark TituloRegion">REGIONES</h2>


<table class="table table-hover">
    <thead class="thead-dark">
        <tr>
            <th scope="col">ID</th>
            <th scope="col">Descripcion</th>            
        </tr>
    </thead>
    <tbody>
        @{
            foreach (var region in Model)
            {
                <tr>
                    <th scope="row">@region.RegionID</th>
                    <th scope="row">@region.RegionDescription</th>           
                </tr>
            }
        }
    </tbody>
</table>





<script src="~/ScriptModel/RegionScript.js"></script>